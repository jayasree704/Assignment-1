array_of_elements = {26, 28, 47, 26, 43, 51, 29}
non_composite_numbers = set()
for number in array_of_elements:
    if number > 1:
        is_prime = True
        for i in range(2, int(number**0.5) + 1):
            if number % i == 0:
                is_prime = False
                break
        if is_prime:
            non_composite_numbers.add(number)
print("Prime numbers in Array elements =", non_composite_numbers)
